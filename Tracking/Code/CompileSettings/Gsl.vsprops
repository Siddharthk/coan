		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="&quot;$(GSL_DIR)\include&quot;"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="libgsl.lib libgslcblas.lib"
