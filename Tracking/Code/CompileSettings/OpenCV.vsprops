	ProjectType="Visual C++"
	Version="8.00"
	Name="OpenCV"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="&quot;$(OPENCV_DIR)\include&quot;"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="cv200.lib cvaux200.lib cxcore200.lib ml200.lib"
		AdditionalLibraryDirectories="&quot;$(OPENCV_DIR)\lib\$(PlatformName)&quot;"
	/>
</VisualStudioPropertySheet>
